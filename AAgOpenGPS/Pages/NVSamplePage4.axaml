<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             xmlns:spg="using:AAgOpenGPS.Pages"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AAgOpenGPS.Pages.NVSamplePage4">
  <ScrollViewer>
    <ui:TabView Name="Other" TabWidthMode="Equal" CloseButtonOverlayMode="Always" TabStripHeader="" TabStripFooter="" CanReorderTabs="False">
      <ui:TabViewItem Header="Heading" IconSource="Document">
        <spg:DHeadingPage/>
      </ui:TabViewItem>
      <ui:TabViewItem Header="Roll" IconSource="Document">
        <spg:DRollPage/>
      </ui:TabViewItem>
    </ui:TabView>
  </ScrollViewer>
</UserControl>
